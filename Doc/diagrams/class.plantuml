@startuml astarun
skinparam class {
	BackgroundColor white
	ArrowColor black
	BorderColor gray
}

abstract class Humain {
	+ nom
	+ prenom
	+ id
}

class MaitreDuJeu {
}

class Joueur {
}

class Cast {
	+ nom
}

class Personnage {
	+ nom
	+ taille
	- mod
}

class Race {
	+ nom
	+ rBonus
	+ uniqueSkill
}
together {
	class Stat {
		+ nom
		+ valeur
		+ rBonus
		+ dBonus
		+ sBonus
		- total
		- mod
		- points
	}

	class Resistance {
		+ elem
		+ valeur
		+ niveau
		+ bonus
		- total
		- points 
	}

	class Jauge {
		+ nom
		+ valeur
		+ bonus
		- total
		- points
	}
}

abstract class Connaissances {
	+ nom
	+ niveau
	+ description
	- points
}
package "" as pckgConn <<Rectangle>> {
	
	class Langue {
		+ nom
		+ points
	}

	class Competence {
		+ bPalier
		- mod
		- total
		- calcPoints()
	}

	class Stances {
		+ modAtk
		+ crit
		+ modCrit
		- calcPoints()
	}

	class Metier {
		- calcPoints()
	}

	class Magie {
		+ elem
		- calcPoints()
	}
}
